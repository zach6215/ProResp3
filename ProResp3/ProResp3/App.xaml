<Application x:Class="ProResp3.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:ProResp3"
             xmlns:views="clr-namespace:ProResp3.Views"
             xmlns:viewmodels="clr-namespace:ProResp3.ViewModels"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <DataTemplate DataType="{x:Type viewmodels:ExperimentViewModel}">
            <views:ExperimentView/>
        </DataTemplate>
        <DataTemplate DataType="{x:Type viewmodels:SetupViewModel}">
            <views:SetupView/>
        </DataTemplate>
    </Application.Resources>
</Application>
